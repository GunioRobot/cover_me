<?xml version="1.0" encoding="UTF-8"?><!-- cover_me report -->

<report>
  <stats>
    <packages value="42"/>
    <classes value="42"/>
    <methods value="42"/>
    <srcfiles value="42"/>
    <srclines value="42"/>
  </stats>
  <data>
    <all name="all classes">
      <coverage type="class, %" value="0%  (0/0)"/>
      <coverage type="method, %" value="0%  (0/0)"/>
      <coverage type="block, %" value="0%  (0/0)"/>
      <coverage type="line, %" value="0%  (<%= global[0] %>/<%= global[1] %>)"/>

      <% reports_by_module.sort.each do |mod,(summary,reports)| %>
      <package name="<%= mod.join('/') %>">
        <coverage type="class, %" value="0%   (0/0)"/>
        <coverage type="method, %" value="0%   (0/0)"/>
        <coverage type="block, %" value="0%   (0/0)"/>
        <coverage type="line, %" value="0%   (<%= summary[0] %>/<%= summary[1] %>)"/>

        <% reports.sort.each do |report| %>
        <srcfile name="<%= report.filename.split(File::SEPARATOR).last %>">
          <coverage type="class, %" value="0%   (0/0)"/>
          <coverage type="method, %" value="0%   (0/0)"/>
          <coverage type="block, %" value="0%   (0/0)"/>
          <coverage type="line, %" value="<%= report.executed_percent.to_i %>%   (<%= report.lines_executed %>/<%= report.lines_of_code %>)"/>
        </srcfile>
        <% end %>
      </package>
      <% end %>
    </all>
  </data>
</report>